
int vis = 0;
int ans = 0;
int mx = 0;

void dfs(long x)
{
    if(x > mx) return;
    ans++;
    for(int i = 0; i < 10; i++)
    {
        if(vis & (1 << i)) continue;
        vis ^= (1 << i);
        dfs(x*10 + i);
        vis ^= (1 << i);
    }
}

int countSpecialNumbers(int n) 
{
    mx = n;
    for(int i = 1; i < 10; i++)
    {
        vis ^= (1 << i);
        dfs(i);
        vis ^= (1 << i);
    }

    return ans;
}
